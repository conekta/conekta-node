    "test": "jest --coverage",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf coverage"
  },
  "dependencies": {
    "axios": "1.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.23",
    "typescript": "^5.4",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ttypescript": "^1.5.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
